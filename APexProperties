public class ApexProperties {

    //encapsulation
    
    private Integer age;
    
    public void setAge(Integer x){
        this.age=x;
    }
        
       public integer getAge(){
           
           return age;
        }
}


///ExecuteAnonymous
ApexProperties c = new ApexProperties();
c.age=30;
c.setAge(25);
Integer age1=c.getAge();
